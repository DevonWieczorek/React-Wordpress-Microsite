{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@Root/*": ["./*"],
      "@Public/*": ["../public/*"],
      "@Core/*": ["./core/*"],
      "@Actions/*": ["./core/actions/*"],
      "@Components/*": ["./core/components/*"],
      "@Modules/*": ["./core/modules/*"],
      "@Pages/*": ["./core/pages/*"],
      "@Plugins/*": ["./plugins/*"],
      "@Reducers/*": ["./core/reducers/*"],
      "@Routes/*": ["./core/routes/*"],
      "@Styles/*": ["./core/styles/*"],
      "@Utils/*": ["./core/utils/*"]
    },
    "target": "ES6",
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true
  }
}
